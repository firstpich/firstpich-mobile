{
  "name": "firstpich",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint --ext .tsx --ext .ts src/",
    "build:tailwind": "tailwindcss --input input.css --output tailwind.css --no-autoprefixer && tailwind-rn",
    "dev:tailwind": "concurrently \"tailwindcss --input input.css --output tailwind.css --no-autoprefixer --watch\" \"tailwind-rn --watch\"",
    "dev:runall": "concurrently \"yarn start\" \"yarn dev:tailwind\" \"yarn android\"",
    "postinstall": "npm run build:tailwind && if which pod > /dev/null; then (cd ios; pod install); fi",
    "kill": "kill $(lsof -t -i:8081)",
    "eslint-check": "eslint \"{src,test}/**/*.tsx\"",
    "eslint-fix": "eslint \"{src,test}/**/*.tsx\" --fix",
    "format": "prettier --check ./src",
    "write": "prettier --write ./src"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@nozbe/watermelondb": "^0.24.0",
    "@nozbe/with-observables": "^1.4.0",
    "@react-native-masked-view/masked-view": "^0.2.6",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/stack": "^6.1.1",
    "@svgr/plugin-svgo": "^6.2.0",
    "graphql": "^16.3.0",
    "jwt-decode": "^3.1.2",
    "react": "17.0.2",
    "react-native": "0.66.4",
    "react-native-dots-pagination": "^0.2.0",
    "react-native-gesture-handler": "~2.1.1",
    "react-native-remix-icon": "^0.3.0",
    "react-native-safe-area-context": "^4.2.1",
    "react-native-screens": "^3.13.1",
    "react-native-simple-toast": "^1.1.3",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^12.1.1",
    "tailwind-rn": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-decorators": "^7.17.8",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@svgr/cli": "^6.2.1",
    "@tailwindcss/typography": "^0.5.2",
    "@types/": "react-navigation/native",
    "@types/jest": "^26.0.23",
    "@types/node": "12",
    "@types/react-native": "^0.66.4",
    "@types/react-native-snap-carousel": "^3.8.5",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "apollo-codegen": "^0.20.2",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "concurrently": "^7.0.0",
    "detox": "^19.4.1",
    "eslint": "^7.14.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.3.7",
    "metro-react-native-babel-preset": "^0.66.2",
    "postcss": "^8.4.6",
    "prettier": "^2.6.1",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "17.0.2",
    "tailwindcss": "^3.0.22",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --ext .tsx --ext .ts src/ --fix"
    ],
    "./src/**": [
      "prettier --write ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
