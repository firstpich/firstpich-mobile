{
  "name": "firstpich",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build:tailwind": "tailwindcss --input input.css --output tailwind.css --no-autoprefixer && tailwind-rn",
    "dev:tailwind": "concurrently \"tailwindcss --input input.css --output tailwind.css --no-autoprefixer --watch\" \"tailwind-rn --watch\"",
    "dev:runall": "concurrently \"yarn start\" \"yarn dev:tailwind\" \"yarn android\"",
    "postinstall": "npm run build:tailwind && if which pod > /dev/null; then (cd ios; pod install); fi",
    "kill": "kill $(lsof -t -i:8081)"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.0",
    "@svgr/plugin-svgo": "^6.2.0",
    "graphql": "^16.3.0",
    "react": "17.0.2",
    "react-native": "0.66.4",
    "react-native-dots-pagination": "^0.2.0",
    "react-native-safe-area-context": "^4.2.1",
    "react-native-screens": "^3.13.1",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^12.1.1",
    "tailwind-rn": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@svgr/cli": "^6.2.1",
    "@tailwindcss/typography": "^0.5.2",
    "@types/jest": "^26.0.23",
    "@types/node": "12",
    "@types/react-native": "^0.66.4",
    "@types/react-native-snap-carousel": "^3.8.5",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "apollo-codegen": "^0.20.2",
    "babel-jest": "^26.6.3",
    "concurrently": "^7.0.0",
    "detox": "^19.4.1",
    "eslint": "^7.14.0",
    "jest": "^27.4.7",
    "metro-react-native-babel-preset": "^0.66.2",
    "postcss": "^8.4.6",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "17.0.2",
    "tailwindcss": "^3.0.22",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
